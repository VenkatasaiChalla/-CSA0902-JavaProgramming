import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        int n=8,c=0,m=n;
        while(n != 0)
        {
            if(n%2 != 0)
            {
                n=n-1;
                c=c+1;
            }
            n/=2;
        }
        System.out.println("No.of 1- bits in "+m+"= "+c);
    }
}
